<div class="panel panel-success">
  <div class="panel-heading">
    <h3 class="panel-title center"><%= @video.title %></h3>
  </div>
  <div class="panel-body center">
    <div class="video-container">
      <iframe width="853" height="505" src="https://www.youtube.com/embed/<%= @video.uid %>" frameborder="0" allowfullscreen>
      </iframe>
    </div>
    <%= link_to "Alle Videos", videos_path, class: "btn btn-danger" %>
  </div>
</div>
